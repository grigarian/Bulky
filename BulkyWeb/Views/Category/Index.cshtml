@model List<Category>

<h1>Список Категорий</h1>
<table class="table table-bordered table-striped"> 
    <thead>
    <tr>
        <th>
            Названия категорий
        </th>
        <th>
            Порядок отображения
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder)){
        <tr>
            <td>@obj.Name</td>
            <td>@obj.DisplayOrder</td>
        </tr>
        }
    </tbody>
</table>
